{
    "extends": "gts/tslint.json",
    "linterOptions": {
      "exclude": [
        "**/*.json"
      ]
    },
    "rules": {
      "variable-name": false,
      "interface-name": false,
      "no-any": false,
      "curly": false,
      "prettier/prettier": [   
        "warn",   {
          "endOfLine": "auto"
        }
      ]
    }
  }